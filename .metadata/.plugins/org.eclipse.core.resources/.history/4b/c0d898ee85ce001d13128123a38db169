#include "stm32l4xx.h"

#define PA5				(1U<<5)
#define LED_PIN			PA5

#define AHB2ENR_GPIOAEN	(1U<<0)
#define MODER_MODE0_0	(1U<<0)
#define MODER_MODE0_1	(1U<<1)


int main(void)
{
	RCC->AHB2ENR |= AHB2ENR_GPIOAEN;

	GPIOA->MODER |= MODER_MODE0_0;
	GPIOA->MODER &= ~MODER_MODE0_1;

	while(1)
	{
		GPIOA->ODR |= LED_PIN;
		for(int i = 0; i < 100000; i++){}
		GPIOA->ODR &= LED_PIN;
		for(int i = 0; i < 100000; i++){}
	}

	return 1;
}
